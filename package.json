{
  "name": "bsky-umbraco-labeler",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "npx tsx src/main.ts",
    "dev": "npx tsx --watch src/main.ts",
    "format": "bunx prettier --write .",
    "lint": "bunx eslint .",
    "lint:fix": "bunx eslint --fix .",
    "set-labels": "bunx tsx src/set-labels.ts",
    "prepare": "bunx husky install"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/better-sqlite3": "^7.6.12",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "eslint": "^9.16.0",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  },
  "dependencies": {
    "@atproto/api": "^0.13.20",
    "@skyware/bot": "^0.3.8",
    "@skyware/labeler": "^0.2.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3"
  }
}
